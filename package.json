{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:env": "NODE_ENV=development tsx -r dotenv/config server/index.ts",
    "dev:doppler": "doppler run --project seed-portal-api --config dev -- npm run dev",
    "dev:api": "SPLIT_DEV=true NODE_ENV=development tsx server/index.ts",
    "dev:web": "vite",
    "dev:web:doppler": "doppler run --project seed-portal-web --config dev -- sh -c 'VITE_STRICT_PORT=1 npm run dev:web'",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:validate": "tsx scripts/validate-build-compression.ts",
    "dev:api:doppler": "doppler run --project seed-portal-api --config dev -- sh -c 'PORT_OVERRIDE=5001 USE_SUPABASE_AUTH=true npm run dev:api'",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:staged": "lint-staged",
    "check:banned-imports": "sh scripts/check-banned-imports.sh",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:smoke": "vitest run __tests__/smoke --reporter=verbose",
    "test:smoke:watch": "vitest __tests__/smoke",
    "test:db:start": "docker-compose -f docker-compose.test.yml up -d && echo '‚è≥ Waiting for database...' && sleep 5",
    "test:db:stop": "docker-compose -f docker-compose.test.yml down",
    "test:db:reset": "npm run test:db:stop && npm run test:db:start",
    "test:db:logs": "docker-compose -f docker-compose.test.yml logs -f postgres-test",
    "test:integration": "npm run test:db:start && vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "npm run test:db:start && vitest --config vitest.integration.config.ts",
    "test:e2e": "doppler run --project seed-portal-web --config dev -- playwright test",
    "test:e2e:ui": "doppler run --project seed-portal-web --config dev -- playwright test --ui",
    "test:e2e:debug": "doppler run --project seed-portal-web --config dev -- playwright test --debug",
    "test:e2e:headed": "doppler run --project seed-portal-web --config dev -- playwright test --headed",
    "test:e2e:chromium": "doppler run --project seed-portal-web --config dev -- playwright test --project=chromium",
    "test:e2e:report": "playwright show-report",
    "test:pricing": "NODE_ENV=development tsx scripts/pricing-tests.ts",
    "test:pricing:golden": "NODE_ENV=development tsx shared/__tests__/pricing.golden.ts",
    "test:pricing:config": "NODE_ENV=development tsx scripts/pricing-config-tests.ts",
    "test:hubspot:sync": "NODE_ENV=development tsx client/src/features/quote-calculator/hooks/__tests__/useHubSpotSync.golden.ts",
    "test:command-dock": "NODE_ENV=development tsx client/src/components/command/__tests__/command-dock.golden.ts",
    "db:push": "drizzle-kit push",
    "db:migrate": "doppler run --project seed-portal-api --config dev -- tsx scripts/run-migration.ts",
    "generate:feature": "tsx scripts/new-feature.ts",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
